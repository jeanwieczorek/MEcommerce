<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="239dp"
        android:layout_height="193dp"
        android:layout_alignStart="@+id/editText11"
        android:layout_below="@+id/editText11"
        app:srcCompat="@mipmap/ic_launcher"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="80dp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:ems="14"
        android:inputType="textPersonName"
        android:text="Pesquisar..."
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editText11"
        android:layout_toEndOf="@+id/editText11"
        app:srcCompat="@android:drawable/ic_search_category_default"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/editText14"
        android:layout_width="100dp"
        android:layout_height="44dp"
        android:layout_below="@+id/imageView7"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="number"
        android:text="1,99"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="229dp" />

    <EditText
        android:id="@+id/editText15"
        android:layout_width="100dp"
        android:layout_height="44dp"
        android:layout_below="@+id/imageView7"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="number"
        android:text="1"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="80dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/editText14"
        android:layout_alignEnd="@+id/editText14"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="264dp"
        tools:layout_editor_absoluteY="137dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="45dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_editor_absoluteX="332dp"
        tools:layout_editor_absoluteY="137dp" />

</android.support.constraint.ConstraintLayout>